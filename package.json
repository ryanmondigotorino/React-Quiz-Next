    "@types/react-dom": "18.0.6",
    "@types/styled-components": "^5.1.26",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "file-loader": "^6.2.0",
    "prisma": "^3.8.1",
    "husky": "^7.0.4",
    "typescript": "4.7.4"
  }
