{
  "name": "toptal-quiz-exam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:save": "dotenv -e .env prisma migrate dev --create-only",
    "migrate:up": "dotenv -e .env prisma migrate deploy",
    "migrate:reset": "dotenv -e .env prisma migrate reset",
    "generate": "dotenv -e .env -- npm -s run generate:prisma",
    "generate:prisma": "prisma generate",
    "db:seed": "dotenv -e .env prisma db seed",
    "prepare": "husky install"
  },
  "dependencies": {
    "@prisma/client": "^3.8.1",
    "@reduxjs/toolkit": "^1.8.4",
    "axios": "^0.27.2",
    "bcryptjs": "^2.4.3",
    "cookie": "^0.5.0",
    "date-fns": "^2.29.1",
    "js-cookie": "^3.0.1",
    "jsonwebtoken": "^8.5.1",
    "jwt-decode": "^3.1.2",
    "next": "12.2.5",
    "next-connect": "^0.13.0",
    "next-seo": "^4.28.1",
    "path": "^0.12.7",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-hook-form": "^3.23.18",
    "react-redux": "^8.0.2",
    "sass": "^1.54.4",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@svgr/webpack": "^6.3.1",
