{
  "name": "toptal-quiz-exam",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "migrate:save": "dotenv -e .env prisma migrate dev --create-only",
    "migrate:up": "dotenv -e .env prisma migrate deploy",
    "migrate:reset": "dotenv -e .env prisma migrate reset",
    "generate": "dotenv -e .env -- npm -s run generate:prisma",
    "generate:prisma": "prisma generate",
    "db:seed": "dotenv -e .env prisma db seed"
  },
  "dependencies": {
    "@prisma/client": "^4.2.1",
    "next": "12.2.5",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@types/node": "18.7.4",
    "@types/react": "18.0.17",
    "@types/react-dom": "18.0.6",
    "dotenv": "^16.0.1",
    "dotenv-cli": "^6.0.0",
    "eslint": "8.22.0",
    "eslint-config-next": "12.2.5",
    "prisma": "^4.2.1",
    "typescript": "4.7.4"
  }
}
